{
  "name": "cli",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "start": "node dist/index.mjs",
    "dev": "rm -rf dist && tsup ./src/index.ts --format=esm --watch --dts --external react",
    "lint": "TIMING=1 eslint \"**/*.ts*\"",
    "lint:ci": "yarn run lint",
    "build": "rm -rf dist && tsup ./src/index.ts --format=esm --dts --external react",
    "build:ci": "yarn run build"
  },
  "bin": {
    "app-cli": "./dist/index.mjs"
  },
  "dependencies": {
    "axios": "^1.2.4",
    "commander": "^9.4.1",
    "dotenv": "^16.0.3",
    "dropbox": "^10.34.0",
    "enquirer": "^2.3.6",
    "file-system-cache": "^2.0.2",
    "pdf-parse": "^1.1.1",
    "promptable": "workspace:*",
    "tslog": "^4.7.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/node-fetch": "^2.6.2",
    "@types/pdf-parse": "^1.1.1",
    "eslint": "7.32.0",
    "ts-node": "^10.9.1",
    "tsconfig": "0.0.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  }
}
